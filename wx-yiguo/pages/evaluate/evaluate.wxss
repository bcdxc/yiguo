.evaluate{
	width: 100%;
	height: 100vh;
	position: relative;
}
.evaluate-head{
	width: 100%;
	height: 30px;
	display: flex;
	justify-content: center;
	align-items: center;
	margin-bottom: 12px;
}
.evaluate-head text{
	font-size: 20px;
	font-weight: 300;
	letter-spacing: 1.5px;
}
.evaluate-main{
	padding: 0 10px;
}
.area-value{
	width: 100%;
	height: 100px;
	border: 1px solid #ccc;
	border-radius: 5px;
	padding: 8px 12px;
	box-sizing: border-box;
	font-size: 16px;
	font-weight: 300;
	letter-spacing: 1.4px;
	margin-top: 20px;
}
.evaluate-main .my-button{
	width: 95px;
	position: absolute;
	bottom: 140px;
	left: 50%;
	transform: translateX(-50%);
}